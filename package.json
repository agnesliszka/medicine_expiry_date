{
  "name": "vue-todolist",
  "description": "A Todo List made with Vue.js",
  "version": "1.0.0",
  "author": "Julian French <ju.french@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy": "npm run build && git add dist/ && git commit -am 'Deploy'"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap-vue": "^2.0.0-rc.11",
    "element-ui": "^2.15.0",
    "express": "^4.17.3",
    "moment": "^2.24.0",
    "serve": "^11.3.2",
    "vue": "^2.5.11",
    "vue-i18n": "^8.22.4",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
